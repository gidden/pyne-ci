x86_64_MacOSX8_remote_pre_declare = conda_fast_build.sh
x86_64_Ubuntu12_remote_pre_declare = conda_fast_build.sh
x86_64_SL6_remote_pre_declare = conda_fast_build.sh
x86_64_Ubuntu14_remote_pre_declare = conda3_build.sh
